  "options": {
    "validateUpsert": true
  },
  "properties": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "string",
      "required": true
    },
    "publico": {
      "type": "boolean",
      "required": false,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "Preguntas": {
      "type": "hasMany",
      "model": "Pregunta",
      "foreignKey": "cuestionarioId",
      "options": {
        "nestRemoting": true
      },
      "through": "PreguntaDelCuestionario"
    },
    "JuegosDeCuestionario": {
      "type": "hasMany",
      "model": "JuegoDeCuestionario",
      "foreignKey": "cuestionarioId"
    },
    "JuegosDeCuestionarioRapido": {
      "type": "hasMany",
      "model": "JuegoDeCuestionarioRapido",
      "foreignKey": "cuestionarioId"
    }
  },
  "acls": [],
  "methods": {}
}
