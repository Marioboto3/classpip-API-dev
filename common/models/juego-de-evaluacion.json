{
  "name": "JuegoDeEvaluacion",
  "plural": "JuegosDeEvaluacion",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombreJuego": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "string",
      "required": true
    },
    "tipo": {
      "type": "string",
      "required": true
    },
    "modo": {
      "type": "string",
      "required": true
    },
    "juegoActivo": {
      "type": "boolean",
      "required": true
    },
    "juegoTerminado": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "profesorEvalua": {
      "type": "boolean",
      "required": true
    },
    "notaProfesorNormal": {
      "type": "boolean",
      "required": false
    },
    "autoEvaluacion": {
      "type": "boolean",
      "required": true
    },
    "evaluadores": {
      "type": "number",
      "required": true
    },
    "pesos": {
      "type": [
        "any"
      ],
      "required": false
    },
    "metodoSubcriterios": {
      "type": "boolean",
      "required": false
    },
    "penalizacion": {
      "type": [
        "any"
      ],
      "required": false
    },
    "preguntasAbiertas": {
      "type": [
        "any"
      ],
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "Equipos": {
      "type": "hasMany",
      "model": "Equipo",
      "foreignKey": "juegoDeEvaluacionId",
      "options": {
        "nestRemoting": true
      },
      "through": "EquipoJuegoDeEvaluacion"
    },
    "Alumnos": {
      "type": "hasMany",
      "model": "Alumno",
      "foreignKey": "juegoDeEvaluacionId",
      "options": {
        "nestRemoting": true
      },
      "through": "AlumnoJuegoDeEvaluacion"
    }
  },
  "acls": [],
  "methods": {}
}
